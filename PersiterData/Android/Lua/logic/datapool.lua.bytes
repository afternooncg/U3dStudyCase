import 'PlayerPrefs'
g_DataPool =
{
	gameplayer = {}
	setMails = {},
	setFriends = {},
}


function g_DataPool:Init()	
	self.IsLoadingOver = false

end


-- 设置登录完成状态
function g_DataPool:SetLoadingState(state)
	self.IsLoadingOver = state
end
-- 设置获取登录完成状态
function g_DataPool:GetLoadingState()
	return self.IsLoadingOver
end


